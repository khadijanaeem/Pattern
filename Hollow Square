#include <iostream>
using namespace std;

int main() {
  // size of the square
  int size = 5;
  // outer loop
  for (int i = 1; i <= size; i++) {
    // inner loop
    for (int j = 1; j <= size; j++) {
      // print only star in first and last row
      if (i == 1 || i == size) {
        cout << "*";
      }
      else {
        // print star only at first and last position row
        if (j == 1 || j == size) {
          cout << "*";
        }
        else {
          cout << " ";
        }
      }
    }
    cout << endl;
  }
  return 0;
}
